{
  "description": "Redirects YouTube URLs to a Firefox extension page",
  "manifest_version": 2,
  "name": "HumaneTube",
  "version": "0.1.0",
  "permissions": [
    "https://*.youtube.com/*",
    "https://*.youtube-nocookie.com/*",
    "https://youtu.be/*",
    "https://*.googlevideo.com/*",
    "tabs",
    "webRequest",
    "webRequestBlocking"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "applications": {
    "gecko": {
      "id": "@social-focus",
      "strict_min_version": "80.0"
    }
  }
}
